config_version=4

_global_script_classes=[ {
"base": "KinematicBody2D",
"class": @"Character",
"language": @"GDScript",
"path": "res://characters/Character.gd"
}, {
"base": "Node2D",
"class": @"IngamePlayer",
"language": @"GDScript",
"path": "res://player/IngamePlayer.gd"
}, {
"base": "Node",
"class": @"PlayerProfile",
"language": @"GDScript",
"path": "res://player/PlayerProfile.gd"
} ]
_global_script_class_icons={
@"Character": "",
@"PlayerProfile": "",
@"IngamePlayer": ""
}

[application]

config/name="Platform-fighter"
run/main_scene="res://stages/Stage.tscn"
config/icon="res://icon.png"

[autoload]

InputScheme="*res://globals/InputScheme.gd"
Global="*res://globals/Global.gd"

[display]

window/size/width=1280
window/size/height=720
window/stretch/aspect="keep"

[input]

p1_test={
"deadzone": 0.5,
"events": [ Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":0,"alt":false,"shift":false,"control":false,"meta":false,"command":false,"pressed":false,"scancode":65,"unicode":0,"echo":false,"script":null)
 ]
}
left={
"deadzone": 0.5,
"events": [ Object(InputEventJoypadMotion,"resource_local_to_scene":false,"resource_name":"","device":0,"axis":0,"axis_value":-1.0,"script":null)
 ]
}

[layer_names]

2d_physics/layer_1="Solid"
2d_physics/layer_2="Platform"
2d_physics/layer_3="Grabs"

[rendering]

environment/default_environment="res://default_env.tres"
